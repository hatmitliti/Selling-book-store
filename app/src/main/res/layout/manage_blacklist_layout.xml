<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:orientation="vertical">    <androidx.appcompat.widget.Toolbar        android:id="@+id/toobar"        android:layout_width="match_parent"        android:layout_height="?attr/actionBarSize"        android:background="@color/cam"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:title="Danh sách đen" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="fill_parent"        android:layout_marginTop="70dp"        android:orientation="vertical">        <Button            android:id="@+id/btnTKBLdanhSachBlackList"            style="@style/buttonStyle"            android:text="Danh Sách Đen" />        <Button            android:id="@+id/btnTKBLthemTaiKhoanVaoBlackList"            style="@style/buttonStyle"            android:text="Thêm Tài Khoản " />    </LinearLayout></LinearLayout>