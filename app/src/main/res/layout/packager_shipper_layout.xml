<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:orientation="vertical">    <androidx.appcompat.widget.Toolbar        android:id="@+id/toobar"        android:layout_width="match_parent"        android:layout_height="?attr/actionBarSize"        android:background="#FF9800"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:title="Các đơn đã đóng gói" />    <TextView        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_weight="1"        android:text="@string/text_shipper_layout"        android:textColor="@color/black"        android:textSize="18sp"        android:gravity="center|start"        android:paddingStart="10dp"        android:textStyle="bold" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_weight="12">        <ListView            android:id="@+id/listPackaged"            android:layout_width="match_parent"            android:layout_height="match_parent" />    </LinearLayout></LinearLayout>